# 🌐 VARIÁVEIS DE AMBIENTE - SISTEMA SOROPEL (PRODUÇÃO)

# 📱 App Configuration  
VITE_APP_NAME=Sistema Soropel
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# 🗄️ Supabase Configuration
# NOTA: VITE_SUPABASE_URL e VITE_SUPABASE_ANON_KEY devem ser configuradas no Dashboard da Vercel
# para manter as chaves seguras e fora do repositório

# 🤖 OpenAI Configuration (para OCR real)
# NOTA: VITE_OPENAI_API_KEY deve ser configurada no Dashboard da Vercel
# para manter a chave segura e fora do repositório

# 🎯 Feature Flags
VITE_ENABLE_OCR=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SUPABASE=true

# 📊 Debug
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error
